    <complexType name="Error">
        <sequence>
            <element name="errorCode" type="xsd:int" minOccurs="1" maxOccurs="1"/>
            <element name="errorDesc" type="xsd:string" minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>
    <complexType name="ErrorStack">
        <sequence>
            <element name="errors" type="tns:Error" minOccurs="0" maxOccurs="unbounded" />
        </sequence>
    </complexType>

    <complexType name="RecordError">
        <sequence>
            <element name="recordID" type="xsd:string" minOccurs="1" maxOccurs="1"/>
            <element name="errorCode" type="xsd:int" minOccurs="1" maxOccurs="1"/>
            <element name="errorDesc" type="xsd:string" minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="RecordErrorList">
        <sequence>
            <element name="errors" type="tns:RecordError" minOccurs="0" maxOccurs="unbounded" />
        </sequence>
    </complexType>

    <complexType name="ResultCodes">
        <sequence>
            <element name="returnCodeEnum" type="tns:ReturnCodeEnum" minOccurs="1" maxOccurs="1"/>
            <element name="statusCodeEnum" type="tns:StatusCodeEnum" minOccurs="1" maxOccurs="1"/>
            <element name="returnCode" type="xsd:int" minOccurs="1" maxOccurs="1"/>
            <element name="statusCode" type="xsd:int" minOccurs="1" maxOccurs="1"/>
        </sequence>
  </complexType>

    <complexType name="AttributeOptions">
        <sequence>
            <element name="negative" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
            <element name="masked" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
            <element name="null" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
            <element name="wildcard" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
        </sequence>
    </complexType>

    <simpleType name="QueryOptionsSortOrderEnum">
        <restriction base="xsd:string">
            <enumeration value="QUERYOPTIONSORTORDER_NONE"/>
            <enumeration value="QUERYOPTIONSORTORDER_ASCENDING"/>
            <enumeration value="QUERYOPTIONSORTORDER_DESCENDING"/>
        </restriction>
    </simpleType>

    <complexType name="QueryOptions">
        <sequence>
            <element name="distinct" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
            <element name="rowoffset" type="xsd:unsignedInt" minOccurs="0" maxOccurs="1"/>
            <element name="rowcount" type="xsd:unsignedInt" minOccurs="0" maxOccurs="1"/>
            <element name="count" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
            <element name="sortorder" type="tns:QueryOptionsSortOrderEnum" minOccurs="0" maxOccurs="1"/>
        </sequence>
    </complexType>

</schema>
